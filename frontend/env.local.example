# Frontend environment variables
# Copy this file to .env.local and fill in the values

# Backend API URL - IMPORTANT: Use your ngrok URL for local development
# Example: https://abcd-efgh-ijkl.ngrok.io
NEXT_PUBLIC_API_URL=http://localhost:8000

# Vapi Public Key - Get this from your Vapi dashboard
# This is different from the secret API key used in the backend
# Example: pk_live_xxxxxxxxxxxxxxxxxxxxxxxx
NEXT_PUBLIC_VAPI_PUBLIC_KEY=your_vapi_public_key_here 